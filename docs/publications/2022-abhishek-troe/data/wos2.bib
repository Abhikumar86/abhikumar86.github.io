@article{ ISI:000305197900017,
Author = {Mashwani, Zia-Ur-Rehman and Khan, Mir Ajab and Ahmad, Mushtaq and Zafar,
   Muhammad and Raja, Naveed Iqbal and Arshad, Muhammad and Samiullah},
Title = {{MACRO-MINERAL QUANTIFICATION OF THE FORAGE GRASS SPECIES IN THE GANDGAR
   HILLS, WESTERN HIMALAYA, PAKISTAN}},
Journal = {{PAKISTAN JOURNAL OF BOTANY}},
Year = {{2012}},
