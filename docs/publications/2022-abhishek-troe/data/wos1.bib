@article{ ISI:000534675600001,
Author = {Nautiyal, Garima and Maithani, Sandeep and Sharma, Archana},
Title = {{Study of Urban Growth Dynamics Using Global Human Settlement Layer Data
   Set: Uttarakhand, India}},
Journal = {{JOURNAL OF THE INDIAN SOCIETY OF REMOTE SENSING}},
Year = {{2020}},
Volume = {{48}},
Number = {{5}},
Pages = {{817-827}},
Month = {{MAY}},
Abstract = {{Due to remote sensing-derived time-series geospatial data sets of
   built-up area, settlement typologies are becoming increasingly available
   in public domain. The present study aims to study the urban growth
   dynamics in Himalayan state of Uttarakhand, India, for the last four
   decades, using open and freely available Global Human Settlement Layer
   data sets. The growth dynamics was analysed based on factors like
   increase in built-up area, hot spot analysis and settlement typologies.
   The results indicate a highly skewed and unbalanced pattern of urban
   growth, especially after the formation of Uttarakhand State in year
   2000. Most of the urban growth process is confined to foothill cites,
   while in the hill towns there is a stagnated urban growth. The study
   emphasizes the applicability of GHSL data sets in analysing the urban
   growth process in Indian conditions where time-series geospatial data
   are often lacking.}},
