@article{ ISI:000373523900011,
Author = {Tudryn, Alina and Abdessadok, Salah and Gargani, Julien and Malasse,
   Anne Dambricourt and Gaillard, Claire and Moigne, Anne-Marie and Sao,
   Cecile Chapon and Singh, Mukesh and Bhardwaj, Vipnesh and Karir, Baldev
   and Miska, Serge},
Title = {{Stratigraphy and paleoenvironment during the Late Pliocene at Masol
   paleonto-archeological site (Siwalik Range, NW India): Preliminary
   results}},
Journal = {{COMPTES RENDUS PALEVOL}},
Year = {{2016}},
