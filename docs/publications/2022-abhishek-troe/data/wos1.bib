@article{ ISI:000490713600031,
Author = {Ahmad, Z. and Aftab, K. and Azad, S. and Khan, M. A.},
Title = {{NEW REMAINS OF MICROSTONYX (SUIDAE, ARTIODACTYLS) FROM THE MIOCENE OF
   PAKISTAN}},
Journal = {{JOURNAL OF ANIMAL AND PLANT SCIENCES}},
Year = {{2019}},
Volume = {{29}},
Number = {{5}},
Pages = {{1485-1490}},
Month = {{OCT}},
Abstract = {{The remains of Microstonyx are discovered during the fossil excavation
   from the Potwar plateau, northern Pakistan. The detailed comparison with
   the Dicoryphochoerus titanoides and Hippopotamodon sivalense, the
   specimens are assigned to Microstonyx major. The genus Microstonyx is
