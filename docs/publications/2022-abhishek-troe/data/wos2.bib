@article{ ISI:000243374400004,
Author = {Schatti, Beat and Schmitz, Andreas},
Title = {{Re-assessing Platyceps ventromaculatus (Gray, 1834) (Reptilia : Squamata
   : Colubrinae)}},
Journal = {{REVUE SUISSE DE ZOOLOGIE}},
Year = {{2006}},
Volume = {{113}},
Number = {{4}},
Pages = {{747-768}},
Month = {{DEC}},
Abstract = {{The morphology and distribution of Gray's racer has been examined.
   Platyceps ventromaculatus seems to frequent habitats below 500 m
   altitude throughout most of its range. Its occurrence in the Himalayan
   foothills (Himachal Pradesh, Uttaranchal) and the specific identity of
   the holotype of P. semifasciatus Blyth, 1860 need further
   investigations. P. ventromaculatus crosses the Indus Valley and is
   found, for instance, along the Makran littoral to as far west as the
   border area with Iran. Coluber chesneii Martin, 1838 may be a valid
   northern Saharo-Arabian taxon of the genus Platyceps Blyth, 1860.}},
