@article{ ISI:000368040500014,
Author = {Kumar, Vishal and Bhandawat, Abhishek and Sharma, Himanshu and Nag,
   Akshay and Sharma, Ram Kumar},
Title = {{Novel microsatellite markers identification and diversity
   characterization in Pteris cretica L.}},
