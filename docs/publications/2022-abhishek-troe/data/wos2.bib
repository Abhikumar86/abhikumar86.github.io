@article{ ISI:000245487200010,
Author = {Akhtar, Mubeena and Hasany, S. Moosa and Bhanger, M. I. and Iqbal,
   Shahid},
Title = {{Sorption potential of Moringa oleifera pods for the removal of organic
   pollutants from aqueous solutions}},
Journal = {{JOURNAL OF HAZARDOUS MATERIALS}},
