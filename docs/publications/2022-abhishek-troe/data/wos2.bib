@article{ ISI:A1996VC57600003,
Author = {Agusti, J and Kohler, M and MoyaSola, S and Cabrera, L and Garces, M and
   Pares, JM},
Title = {{Can Llobateres: The pattern and timing of the Vallesian hominoid
   radiation reconsidered}},
Journal = {{JOURNAL OF HUMAN EVOLUTION}},
Year = {{1996}},
Volume = {{31}},
Number = {{2}},
Pages = {{143-155}},
Month = {{AUG}},
Abstract = {{New biostratigraphic data and paleomagnetic analysis of the site of Can
   Llobateres (Northeastern Spain) allows an age-estimation of 9.6 Ma for
   the recently discovered Dryopithecus laietanus facial remains
   (CL1-18000) and 9.7 Ma for the earlier hominoid finds from this
   locality. This is an unexpectedly young age for such a primitive form,
   which can be accurately correlated with some of the finds of youngest
   Sivapithecus in the Silwaliks. According to the phylogenetic model
   proposed here, Dryopithecus displayed along period (some 5 Ma) of
   morphological stasis in Europe, which could be related to the
   preservation of subtropical forest conditions in the Western
   Mediterranean. In other parts of Southern Eurasia (Eastern Mediterranean
   and Siwaliks), quick vicarious evolution led to the diversification of
   thick-enamelled hominoids between 14 Ma and 12 Ma. (C) Academic Press
   Limited}},
