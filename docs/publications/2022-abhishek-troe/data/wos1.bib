@article{ ISI:000526679900008,
Author = {Tuti, Mangal Deep and Pal, Ramesh Singh and Mahanta, Dibakar and Pandey,
   Brij Mohan and Bisht, Jaideep Kumar},
Title = {{Soil Chemical and Biological Activities under Vegetable Intensive
   Colocasia-based Cropping System in Indian Sub-Himalayas}},
