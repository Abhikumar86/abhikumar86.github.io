@article{ ISI:000262251800012,
Author = {Khan, M. A. and Khan, A. M. and Iqbal, M. and Akhtar, M. and Mahboob, S.},
Title = {{REDUNCINE FOSSILS FROM THE UPPER SIWALIKS OF TATROT}},
Journal = {{JOURNAL OF ANIMAL AND PLANT SCIENCES}},
Year = {{2008}},
Volume = {{18}},
Number = {{1}},
Pages = {{50-52}},
Abstract = {{Fossil remains of the reduncines are preserved in the Siwalik Tatrot
   Formation of northern Pakistan. The Tatrot fauna is dominated by the
   bovids, represent most abundant taxa found from the Siwaliks. These
   specimens were discovered by the first author in the well dated late
   Pliocene deposits of the Potwar Plateau, northern Pakistan. The Pliocene
   deposits of the Tatrot Formation yielded fossil remains of reduncines,
   which are now grazing antelopes found in habitats associated with the
   vicinity of water. This discovery adds weight to the suggestion that the
   Pliocene reduncines inhabited the riverine galleys rather than the dry
   regions and moreover the sedimentary deposits of the Tatrot were the
   wetland area during the Pliocene epoch that documents the ancient biome.}},
