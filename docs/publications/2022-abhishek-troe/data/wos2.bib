@article{ ISI:000178491300015,
Author = {Nayal, JS and Thapliyal, RC and Phartyal, SS and Joshi, G},
Title = {{Germination and storage behaviour of seeds of Grewia optiva (Tiliaceae)
   - a sub-tropical Himalayan multipurpose evergreen tree}},
Journal = {{SEED SCIENCE AND TECHNOLOGY}},
Year = {{2002}},
Volume = {{30}},
Number = {{3}},
Pages = {{629-639}},
Abstract = {{The Grewia optiva seed has a bony, hard seedcoat that does not appear to
   restrict the germination of seed and hence does not need any
   pre-treatment. The temperature around 30degreesC and BP media had
   significantly better effect on the germination and other parameter, The
   seeds stored with high moisture content (17\%) in airtight container
   maintained viability for longer periods as compared to vacuum container.
   However, the seeds desiccated to 5\% me maintained better storability in
   vacuum container than the air-tight container. Seed me plays a
   significant role in storage environment, The reduction in seed me down
   to 5\% resulted in a significant increase in p(50) in airtight as well
   as in vacuums containers at all temperatures. On the other hand
   temperature indicates negative logarithmic relation with seed longevity.
   The data recorded for the relationship between temperature and seed me
   during storage was fitted to the basic viability equation and constants
   were calculated which besides proving the orthodox nature of seed of
   this species can predict seed longevity in a given condition of seed me
   and storage temperature for a particular seed lot.}},
