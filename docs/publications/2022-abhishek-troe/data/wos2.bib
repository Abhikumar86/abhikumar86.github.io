@article{ ISI:000221830700001,
Author = {Kunimatsu, Y and Ratanasthien, B and Nakaya, H and Saegusa, H and
   Nagaoka, S},
Title = {{Earliest Miocene Hominoid from Southeast Asia}},
Journal = {{AMERICAN JOURNAL OF PHYSICAL ANTHROPOLOGY}},
