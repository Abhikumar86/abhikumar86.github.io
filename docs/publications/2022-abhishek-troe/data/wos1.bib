Author = {Xafis, Alexandros and Mayda, Serdar and Grimsson, Friogeir and Nagel,
   Doris and Kaya, Tanju and Halaclar, Kazim},
Title = {{Fossil Giraffidae (Mammalia, Artiodactyla) from the early Turolian of
   Kavakdere (Central Anatolia, Turkey)}},
Journal = {{COMPTES RENDUS PALEVOL}},
