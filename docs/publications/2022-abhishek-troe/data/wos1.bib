@article{ ISI:000562844100001,
Author = {Khan, Shahzeb Ullah and Zafar, Muhammad and Ullah, Riaz and Shahat,
   Abdelaaty A. and Ahmad, Mushtaq and Sultana, Shazia and Malik, Khafsa},
