@article{ ISI:000175205800002,
Author = {Negi, HR and Gadgil, M},
Title = {{Cross-taxon surrogacy of biodiversity in the Indian Garhwal Himalaya}},
Journal = {{BIOLOGICAL CONSERVATION}},
