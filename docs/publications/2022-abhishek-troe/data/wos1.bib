@article{ ISI:000615603400001,
Author = {Mahmood, Taufique H. and Putkonen, Jaakko and Sobbe, Aaron},
Title = {{Spatially Variable Precipitation and Its Influence on Water Balance in a
   Headwater Alpine Basin, Nepal}},
