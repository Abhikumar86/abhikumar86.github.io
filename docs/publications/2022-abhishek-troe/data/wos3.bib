@article{ ISI:A1992HE09900005,
Author = {RAHMAN, MA and WILCOCK, CC},
Title = {{DIVERSITY OF LIFE-FORM AND DISTRIBUTION OF THE ASCLEPIADACEAE IN
   SOUTH-WEST ASIA AND THE INDIAN SUBCONTINENT}},
Journal = {{JOURNAL OF BIOGEOGRAPHY}},
Year = {{1992}},
Volume = {{19}},
Number = {{1}},
Pages = {{51-58}},
Month = {{JAN}},
Abstract = {{The Asclepiadaceae of south-west Asia and the Indian subcontinent were
   analysed for their variation in life-form, distribution and percentage
   of endemic taxa in the different countries of the region.  A total of
   327 species from fifty-one genera were recognized.  There is a major
   development of the family in India and the Himalayan foothills.  The
   family exhibits a wide diversity of life-form in the region:  herbs,
   shrubs, succulents, epiphytes, climbers and twiners.  Succulent forms
   predominate in the Arabian peninsula and herbs in Turkey.  In the rest
   of SW Asia the predominant life-forms are shrubs and herbs.  Woody
   twiners and climbers increase in importance eastwards, forming the
   dominant life-forms of the Indian subcontinent.}},
