@article{ ISI:000475326400010,
Author = {Kabra, Atul and Martins, Natalia and Sharma, Rohit and Kabra, Ruchika
   and Baghel, Uttam Singh},
Title = {{yyy Myrica esculenta Buch.-Ham. ex D. Don: A Natural Source for Health
   Promotion and Disease Prevention}},
