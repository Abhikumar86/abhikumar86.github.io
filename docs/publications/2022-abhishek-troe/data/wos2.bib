@article{ ISI:000239237400004,
Author = {Parmar, Varun and Prasad, Guntupalli V. R.},
Title = {{Middle Miocene rhizomyid rodent (Mammalia) from the Lower Siwalik
   Subgroub of Ramnagar, Udhampur District, Jammu and Kashmir, India}},
Journal = {{NEUES JAHRBUCH FUR GEOLOGIE UND PALAONTOLOGIE-MONATSHEFTE}},
Year = {{2006}},
Number = {{6}},
Pages = {{371-384}},
Month = {{JUN}},
Abstract = {{An incomplete right dentary of a rhizomyid rodent, bearing M-2 - M-3, is
   described for the first time from the Lower Siwalik deposits of Ramnagar
   area, NW India. The size and occlusal morphology of the M-2 and M-3
   compare well with that of Kanisamys potwarensis FLYNN, 1982 known from
   the Middle Miocene (14.3-13.3 Ma) Chinji Formation of Potwar Plateau,
   Pakistan. The recovery of the dentary closely resembling K. potwarensis
   from the Lower Siwaliks of India confirms the presence of a uniform
   ecological realm in the Middle Miocene in the Siwalik belt of India and
   Pakistan.}},
