@article{ ISI:000470336100005,
Author = {Talukdar, Nazimur Rahman and Choudhury, Parthankar and Singh, Biswajit},
Title = {{Current records of porcupine in northeast India: distribution, habitat
   preference and conservation}},
Journal = {{TROPICAL ECOLOGY}},
Year = {{2019}},
Volume = {{60}},
Number = {{1}},
Pages = {{41-51}},
Month = {{MAR}},
Abstract = {{Porcupines are found in different parts of Northeast India. Three
   species of porcupines have been reported from northeast India. Except
   forsome discrete reports, the detailed distribution of porcupine has not
   yet been studied. To have a clear idea of the present distribution of
   porcupines in northeast India, their habitat preference, all available
   reports and literature have been reviewed. The study revealed Himalayan
   porcupine is distributed all over northeast India and have wide
   environmental tolerance from tropical tothe temperate forest and
   sub-montane forest. Several anthropogenic threats including poaching for
   its delicious meat have reduced its population at an alarming rate.
   Priority should be given to the conservation of the species before
   reaching to endangered level.}},
