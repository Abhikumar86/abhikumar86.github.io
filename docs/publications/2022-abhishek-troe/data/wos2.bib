@article{ ISI:000346690700018,
Author = {Hossain, H. M. Zakir and Tarek, Mostafa and Armstrong-Altrin, John S.
   and Monir, Md. Minhaj Uddin and Ahmed, Mohammad Tofayal and Ahmed, Syed
   Istiyak and Janete Hernandez-Coronado, Claudia},
Title = {{MICROTEXTURES OF DETRITAL SAND GRAINS FROM THE COX'S BAZAR BEACH,
   BANGLADESH: IMPLICATIONS FOR PROVENANCE AND DEPOSITIONAL ENVIRONMENT}},
Journal = {{CARPATHIAN JOURNAL OF EARTH AND ENVIRONMENTAL SCIENCES}},
Year = {{2014}},
Volume = {{9}},
Number = {{3}},
Pages = {{187-197}},
Month = {{AUG}},
Abstract = {{Surface features on detrital sand grains were studied by a scanning
   electron microscope (SEM) to infer the provenance, depositional
   environment and paleoclimate. Twelve sand samples were collected from
   Laboni, Sugandha and Himchori points along the Cox's Bazar beach,
   Bangladesh. Twenty six microtextures were identified and classified as
   mechanical (16 features), mechanical/chemical (5 features) and chemical
   (5 features) origins. The microtextural features of sand grains were
   characterized by straight scratches, v-shaped patterns (Vs), smooth
   surfaces with sub-angular to sub-rounded outlines. Conchoidal fractures,
   Vs and sub-angular to sub-rounded grains indicate mechanical origin. The
   sub-rounded to rounded grains suggesting long-distance transportation
   under fluvial environment (e.g. Himalaya). The abundance of sand grains
   with angular to sub-angular outlines revealed a short transportation and
   derived from the nearby Chittagong-Tripura folded belt and/or
   Indo-Burman Ranges. Etching in the sand grains represents chemical
   origin under the influence of sea-water.}},
