@article{ ISI:000367636700013,
Author = {Tiwari, S. and Dumka, U. C. and Kaskaoutis, D. G. and Ram, Kirpa and
   Panicker, A. S. and Srivastava, M. K. and Tiwari, Shani and Attri, S. D.
   and Soni, V. K. and Pandey, A. K.},
Title = {{Aerosol chemical characterization and role of carbonaceous aerosol on
   radiative effect over Varanasi in central Indo-Gangetic Plain}},
Journal = {{ATMOSPHERIC ENVIRONMENT}},
Year = {{2016}},
Volume = {{125}},
Number = {{B, SI}},
Pages = {{437-449}},
Month = {{JAN}},
Abstract = {{This study investigates the chemical composition of PM10 aerosols at
   Varanasi, in the central Indo-Gangetic Plain (IGP) during April to July
   2011, with emphasis on examining the contribution of elemental carbon
   (EC) to the estimates of direct aerosol radiative effect (DARE). PM10
   samples are analysed for carbonaceous aerosols (Organic Carbon, OC and
   EC) and water-soluble ionic species (WSIS: Cl-, SO42-, NO3-, PO42- NH4+,
   Na+, K+, Mg2+ and Ca2+) and several diagnostic ratios (OC/EC, K+/EC,
   etc) have been also used for studying the aerosol sources at Varanasi.
   PM10 mass concentration varies between 53 and 310 mu m(-3)(mean of 168
   73 mu g m(-3)), which is much higher than the National and International
   air quality standards. The OC mass concentration varies from 6 mu g
   m(-3) to 24 mu g m(-3) (mean of 12 +/- 5 mu g m(-3); 7\% of PM10 mass),
   whereas EC ranges between 1.0 and 14.3 mu g m(-3) (4.4 +/- 3.9 rig
   m(-3); similar to 3\% of PM()mass). The relative low OC/EC of 3.9 +/-
   2.0 and strong correlation (R-2 = 0.82) between them suggest the
   dominance of primary carbonaceous aerosols. The contribution of WSIS to
   PM10 is found to be similar to 12\%, out of which similar to 57\% and
   43\% are anions and cations, respectively. The composite DARE estimates
   via SBDART model reveal significant radiative effect and atmospheric
   heating rates (0.9-2.3 K day(-1)). Although the EC contributes only
   similar to 3\% to the PM10 mass, its contribution to the surface and
   atmospheric forcing is significantly high (37-63\% and 54-77\%,
   respectively), thus playing a major role in climate implications over
   Varanasi. (C) 2015 Elsevier Ltd. All rights reserved.}},
