@article{ ISI:000336314900041,
Author = {Bhuyan, Pradip Kumar and Bharali, Chandrakala and Pathak, Binita and
   Kalita, Gayatry},
Title = {{The role of precursor gases and meteorology on temporal evolution of O-3
   at a tropical location in northeast India}},
