@article{ ISI:000333119900017,
Author = {Manjunath, K. R. and Kumar, Amit and Meenakshi, Mehra and Renu, R. and
   Uniyal, S. K. and Singh, R. D. and Ahuja, P. S. and Ray, S. S. and
   Panigrahy, Sushma},
Title = {{Developing Spectral Library of Major Plant Species of Western Himalayas
   Using Ground Observations}},
