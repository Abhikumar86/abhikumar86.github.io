@article{ ISI:000511847200003,
Author = {Wani, Ahsan Afzal and Bali, Bikram Singh and Bhat, G. R. and Hussain,
   Nasir},
Title = {{Impact of tectonics on drainage network evolution of Suru basin, Kargil
   N/w Himalaya, Jammu and Kashmir, India}},
