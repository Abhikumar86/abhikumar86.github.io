@article{ ISI:A1994NW15100006,
Author = {SUNDRIYAL, RC and SHARMA, E and RAI, LK and RAO, SC},
Title = {{TREE STRUCTURE, REGENERATION AND WOODY BIOMASS REMOVAL IN A SUBTROPICAL
   FOREST OF MAMLAY WATERSHED IN THE SIKKIM HIMALAYA}},
