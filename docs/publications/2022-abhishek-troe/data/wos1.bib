@article{ ISI:000392489400067,
Author = {Vijay, Saurabh and Braun, Matthias},
Title = {{Elevation Change Rates of Glaciers in the Lahaul-Spiti (Western
   Himalaya, India) during 2000-2012 and 2012-2013}},
